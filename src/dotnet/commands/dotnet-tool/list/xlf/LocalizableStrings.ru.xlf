<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="ru" original="../LocalizableStrings.resx">
    <body>
      <trans-unit id="CommandDescription">
        <source>List tools installed globally or locally.</source>
        <target state="needs-review-translation">Вывод списка инструментов, установленных в текущей среде разработки.</target>
        <note />
      </trans-unit>
      <trans-unit id="GlobalOptionDescription">
        <source>List tools installed for the current user.</source>
        <target state="needs-review-translation">Вывод списка инструментов в каталоге инструментов текущего пользователя.</target>
        <note />
      </trans-unit>
      <trans-unit id="ListToolCommandInvalidGlobalAndLocalAndToolPath">
        <source>The local option(--local), the global option (--global), the tool path option (--tool-path), can only have one at a time. Please specify only one of the options: {0}.</source>
        <target state="new">The local option(--local), the global option (--global), the tool path option (--tool-path), can only have one at a time. Please specify only one of the options: {0}.</target>
        <note />
      </trans-unit>
      <trans-unit id="LocalOptionDescription">
        <source>List the tools installed in the local tool manifest.</source>
        <target state="new">List the tools installed in the local tool manifest.</target>
        <note />
      </trans-unit>
      <trans-unit id="ManifestFileColumn">
        <source>Manifest</source>
        <target state="new">Manifest</target>
        <note />
      </trans-unit>
      <trans-unit id="VersionColumn">
        <source>Version</source>
        <target state="translated">Версия</target>
        <note />
      </trans-unit>
      <trans-unit id="CommandsColumn">
        <source>Commands</source>
        <target state="translated">Команды</target>
        <note />
      </trans-unit>
      <trans-unit id="PackageIdColumn">
        <source>Package Id</source>
        <target state="translated">Идентификатор пакета</target>
        <note />
      </trans-unit>
      <trans-unit id="InvalidPackageWarning">
        <source>Warning: tool package '{0}' is invalid, please contact tool author:
{1}</source>
        <target state="needs-review-translation">Предупреждение. Пакет инструментов "{0}" недопустим: {1}</target>
        <note />
      </trans-unit>
      <trans-unit id="InvalidToolPathOption">
        <source>Tool path '{0}' does not exist.</source>
        <target state="translated">Путь к средству "{0}" не существует.</target>
        <note />
      </trans-unit>
      <trans-unit id="ToolPathOptionName">
        <source>PATH</source>
        <target state="translated">PATH</target>
        <note />
      </trans-unit>
      <trans-unit id="ToolPathOptionDescription">
        <source>The directory containing the tools to list.</source>
        <target state="translated">Каталог, содержащий перечисляемые инструменты.</target>
        <note />
      </trans-unit>
